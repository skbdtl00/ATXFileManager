{
  "name": "atxfilemanager",
  "version": "1.0.0",
  "description": "Advanced File Manager with cloud storage integration, automation, and enterprise features",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage",
    "migrate": "node dist/database/migrate.js",
    "seed": "node dist/database/seed.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install"
  },
  "keywords": [
    "file-manager",
    "cloud-storage",
    "s3",
    "ftp",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "node-7z": "^3.0.0",
    "sharp": "^0.33.1",
    "pdf-lib": "^1.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "aws-sdk": "^2.1519.0",
    "ftp": "^0.3.10",
    "ssh2-sftp-client": "^10.0.3",
    "node-schedule": "^2.1.1",
    "cron-parser": "^4.9.0",
    "clamscan": "^2.2.1",
    "socket.io": "^4.6.0",
    "winston": "^3.11.0",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-jwt": "^4.0.1",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "nodemailer": "^6.9.7",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "cookie-parser": "^1.4.6"
